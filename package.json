{
	"name": "todotools",
	"displayName": "ToDoTools",
	"publisher": "grahampcharles",
	"description": "Adds auxilary functions for taskpaper documents.",
	"version": "0.1.2",
	"repository": {
		"type": "git",
		"url": "git://github.com/grahampcharles/VSCode-todotools.git"
	},
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:todotools.copyDailyToToday",
		"onCommand:todotools.runOnOpen",
		"onLanguage:todo"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "todotools.copyDailyToToday",
				"title": "ToDo Tools: Copy Daily Tasks to Today"
			},
			{
				"command": "todotools.runOnOpen",
				"title": "ToDo Tools: Simulate On Open"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.52.0",
		"@types/yamljs": "^0.2.31",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"eslint": "^7.15.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1"
	},
	"dependencies": {
		"dayjs": "^1.10.7",
		"yaml": "^1.10.2",
		"yamljs": "^0.3.0"
	}
}
